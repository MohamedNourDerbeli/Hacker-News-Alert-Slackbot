{
    "display_information": {
        "name": "Hacker News Alerts",
        "description": "Get alerts for keywords in top Hackernews Stories",
        "background_color": "#000000"
    },
    "features": {
        "bot_user": {
            "display_name": "Hacker News Alerts",
            "always_online": false
        }
    },
    "oauth_config": {
        "scopes": {
            "bot": [
                "chat:write",
                "im:write"
            ]
        }
    },
    "settings": {
        "interactivity": {
            "is_enabled": true
        },
        "org_deploy_enabled": false,
        "socket_mode_enabled": true,
        "token_rotation_enabled": false
    }
}